# 203-BobTreatmentValue.rq
# Return Bob's treatment. No visualization
PREFIX phuse: <http://www.example.org/phuse/workshop/>
SELECT  ?treatment 
WHERE {
    ?person phuse:hasFirstName "Bob" .
    ?person phuse:hasTreatment ?treat .
    # Make the result pretty
    BIND (strafter(str(?treat), "workshop/") AS ?treatment)
}