# 240-NamesInTreatmentType.rq  Names of Persons in each type of treatment 
SELECT ?treatArmType ?personName
WHERE {
  # Easy part: names all people
  ?person schema:givenName ?personName; 
          # Follow the path from person to treatment type
          eg:randomizedTo  ?treatmentArm .
  ?treatmentArm eg:treatmentArmType ?treatmentArmType .
}
ORDER BY ?treatmentArmType ?personName